@{
    ViewData["Title"] = "Authenticated";
}
<h1>@ViewData["Title"]</h1>

<dl>
    @foreach (var claim in this.User.Claims)
    {
        <dt>@claim.Type</dt>
        <dd>@claim.Value</dd>
    }
</dl>

<hr/>

<ul>
    @foreach (var claim in this.User.Claims)
    {
        <li><strong>@claim.Type</strong>: @claim.Value</li>
    }
</ul>
